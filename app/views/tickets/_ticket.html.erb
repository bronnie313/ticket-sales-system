<div id="<%= dom_id ticket %>">
  <p>
    <strong>Name:</strong>
    <%= ticket.name %>
  </p>

  <p>
    <strong>Seat:</strong>
    <%= ticket.seat_id %>
  </p>

  <p>
    <strong>Address:</strong>
    <%= ticket.address %>
  </p>

  <p>
    <strong>Price paid:</strong>
    <%= ticket.price_paid %>
  </p>

  <p>
    <strong>Email address:</strong>
    <%= ticket.email_address %>
  </p>

  <p>
    <strong>Phone number:</strong>
    <%= ticket.phone %>
  </p>

</div>
